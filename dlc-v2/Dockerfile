FROM registry.git.embl.org/grp-cbbcs/abcdesktop-apps/base-image:ubuntu22-cudnn8-cuda-11-7

ENV PATH="/opt/conda/bin:/usr/local/cuda/bin:${PATH}"
ARG PATH="/opt/conda/bin:/usr/local/cuda/bin:${PATH}"
ENV LD_LIBRARY_PATH="/opt/conda/envs/dlc/libs:$LD_LIBRARY_PATH"
ENV NVIDIA_DRIVER_CAPABILITIES "compute,utility"

RUN DEBIAN_FRONTEND=noninteractive apt-get update \
    && apt-get install --no-install-recommends --yes build-essential python3.10 python3-pip python-is-python3 curl libcurl4-openssl-dev libssl-dev software-properties-common wget libarchive-dev git openssh-client telnet netcat sshcommand sshfs ftp-ssl wput tftp ncftp git git-ftp ftp dbus-x11 libcanberra-gtk-module libcanberra-gtk3-module konsole libglib2.0-0 libgl1 make\
    && apt-get clean \ 
    && add-apt-repository -y ppa:mozillateam/ppa \ 
    && apt install -y firefox-esr \
    && echo 'debconf debconf/frontend select Noninteractive' | debconf-set-selections


RUN  wget https://github.com/conda-forge/miniforge/releases/latest/download/Miniforge3-Linux-x86_64.sh \
    && bash Miniforge3-Linux-x86_64.sh -b -p /opt/conda \
    && rm -f Miniforge3-Linux-x86_64.sh \
    && conda install mamba -n base -c conda-forge

########
RUN mamba create --name dlc --yes python=3.10 pytables==3.8.0 -c conda-forge \
    && /opt/conda/envs/dlc/bin/pip install torch==2.5.1 torchvision==0.20.1 torchaudio==2.5.1 --index-url https://download.pytorch.org/whl/cu118 \
    && /opt/conda/envs/dlc/bin/pip install numpy==1.23.5 huggingface_hub wandb tensorflow[and-cuda]==2.12 "tensorpack>=0.11" "tf_slim>=1.1.0" \
    && /opt/conda/envs/dlc/bin/pip install --pre "deeplabcut[gui,tf]==2.3.11"

COPY startdlc2.sh /usr/local/bin/startdlc2.sh

RUN mkdir -p /opt/conda/envs/dlc/lib/python3.10/site-packages/deeplabcut/pose_estimation_pytorch/models/backbones/pretrained_weights && \
    chmod a+rwx -R /opt/conda/envs/dlc/lib/python3.10/site-packages/deeplabcut/pose_estimation_pytorch/models/backbones/pretrained_weights && \
    wget https://download.pytorch.org/models/resnet18-f37072fd.pth -O /opt/conda/envs/dlc/lib/python3.10/site-packages/deeplabcut/pose_estimation_pytorch/models/backbones/pretrained_weights/resnet18.pth && \
    wget https://download.pytorch.org/models/resnet34-b627a593.pth -O /opt/conda/envs/dlc/lib/python3.10/site-packages/deeplabcut/pose_estimation_pytorch/models/backbones/pretrained_weights/resnet34.pth && \
    wget https://download.pytorch.org/models/resnet50-0676ba61.pth -O /opt/conda/envs/dlc/lib/python3.10/site-packages/deeplabcut/pose_estimation_pytorch/models/backbones/pretrained_weights/resnet50.pth && \
    wget https://download.pytorch.org/models/resnet101-63fe2227.pth -O /opt/conda/envs/dlc/lib/python3.10/site-packages/deeplabcut/pose_estimation_pytorch/models/backbones/pretrained_weights/resnet101.pth && \
    wget https://download.pytorch.org/models/resnet152-394f9c45.pth -O /opt/conda/envs/dlc/lib/python3.10/site-packages/deeplabcut/pose_estimation_pytorch/models/backbones/pretrained_weights/resnet152.pth && \
    chmod +x /usr/local/bin/startdlc2.sh


WORKDIR /opt/conda/envs/dlc/lib/python3.10/site-packages/deeplabcut/pose_estimation_tensorflow/models/pretrained

RUN wget http://download.tensorflow.org/models/resnet_v1_50_2016_08_28.tar.gz && \
    tar xzvf resnet_v1_50_2016_08_28.tar.gz && \
    wget http://download.tensorflow.org/models/resnet_v1_101_2016_08_28.tar.gz && \
    tar xzvf resnet_v1_101_2016_08_28.tar.gz && \
    wget http://download.tensorflow.org/models/resnet_v1_152_2016_08_28.tar.gz && \
    tar xzvf resnet_v1_152_2016_08_28.tar.gz && \
    rm resnet_v1_50_2016_08_28.tar.gz resnet_v1_101_2016_08_28.tar.gz resnet_v1_152_2016_08_28.tar.gz


LABEL oc.icon="dlc.svg"
LABEL oc.icondata=""
LABEL oc.keyword="DeepLabCut"
LABEL oc.cat="development"
LABEL oc.desktopfile="org.kde.konsole.desktop"
LABEL oc.launch="dlc2.konsole"
LABEL oc.template="abcdesktopio/oc.template.ubuntu.nvidia.22.04"
LABEL oc.name="dlc2"
LABEL oc.displayname="DeepLabCut 2"
LABEL oc.path="/usr/bin/konsole"
LABEL oc.args="--hold -e bash --rcfile /usr/local/bin/startdlc2.sh "
LABEL oc.type=app
LABEL oc.rules="{\"homedir\":{\"default\":true}}"
LABEL oc.acl="{\"permit\":[\"all\"]}"

RUN  if [ -d /usr/share/icons ]   && [ -x /composer/safelinks.sh ] && [ -d /usr/share/icons   ];  then cd /usr/share/icons;    /composer/safelinks.sh; fi \
    && if [ -d /usr/share/pixmaps ] && [ -x /composer/safelinks.sh ] && [ -d /usr/share/pixmaps ];  then cd /usr/share/pixmaps;  /composer/safelinks.sh; fi 
ENV APPNAME "dlc2"
ENV APPBIN "/usr/bin/konsole"
ENV APP "/usr/bin/konsole"
ENV ARGS="--hold -e bash --rcfile /usr/local/bin/startdlc2.sh"

ENV LD_LIBRARY_PATH "/usr/local/cuda/lib64:${LD_LIBRARY_PATH}",
ENV NVIDIA_DRIVER_CAPABILITIES "compute,utility"

RUN mkdir -p /run/user \
    && chmod 777 /run/user \
    && mkdir -p /etc/localaccount \
    && for f in passwd shadow group gshadow ; do if [ -f /etc/$f ] ; then  cp /etc/$f /etc/localaccount; rm -f /etc/$f; ln -s /etc/localaccount/$f /etc/$f; fi; done

CMD [ "/composer/appli-docker-entrypoint.sh" ]

