ARG TAG=3.2
FROM abcdesktopio/oc.template.ubuntu.22.04:$TAG

ARG DEBIAN_FRONTEND="noninteractive"

ENV LANG en_US.UTF-8 \
    LC_ALL en_US.UTF-8 \
    LANGUAGE en_US:en  \
    NUMBA_CACHE_DIR=/tmp


RUN apt-get update -y && \
    apt-get install -y -q --no-install-recommends \
            gcc \
            wget \
            qtcreator \
            python3-dev \
            python3-pip \
            python3-wheel \
            libblas-dev \
            liblapack-dev \
            libgl1 \
            mesa-utils \
            libxcb-cursor0 \
            libgl1-mesa-glx \
            libxcb-xinerama0 \
            libatlas-base-dev \
            gfortran \
            apt-utils \
            bzip2 \
            ca-certificates \
            curl \
            locales \
            libarchive-dev \
            cmake \
            konsole \
            unzip &&  apt-get clean


RUN rm -rf /var/lib/apt/lists/* 


RUN sed -i '/en_US.UTF-8/s/^# //g' /etc/locale.gen && \
    locale-gen


WORKDIR /tmp
RUN wget https://github.com/conda-forge/miniforge/releases/latest/download/Miniforge3-Linux-x86_64.sh \
    && bash Miniforge3-Linux-x86_64.sh -b -p /opt/conda \
    && rm -f Miniforge3-Linux-x86_64.sh 


LABEL oc.icon="cellpose.svg"
LABEL oc.icondata=""
LABEL oc.keyword="Cellpose2"
LABEL oc.cat="development"
LABEL oc.template="abcdesktopio/oc.template.gtk"
LABEL oc.name="cellpose2"
LABEL oc.launch="cellpose2.konsole"
LABEL oc.displayname="Cellpose 2.3.2(Headless)"
LABEL oc.path="/usr/bin/konsole"
LABEL oc.type=app
LABEL oc.rules="{\"homedir\":{\"default\":true}}"
LABEL oc.acl="{\"permit\":[\"all\"]}"

RUN  if [ -d /usr/share/icons ];   then cd /usr/share/icons;    /composer/safelinks.sh; fi 
RUN  if [ -d /usr/share/pixmaps ]; then cd /usr/share/pixmaps;  /composer/safelinks.sh; fi 

ENV APPNAME "cellpose2"
ENV APPBIN "/usr/bin/konsole"
ENV APP "/usr/bin/konsole"
ENV CONDA_BIN_PATH="/opt/conda/bin"
ENV PATH $CONDA_BIN_PATH:$PATH
ENV LD_PRELOAD="/usr/lib/x86_64-linux-gnu/libstdc++.so.6"
ENV LD_LIBRARY_PATH "/usr/local/nvidia/lib:/usr/local/nvidia/lib64"
ENV NVIDIA_DRIVER_CAPABILITIES "compute,utility"

ENV QT_DEBUG_PLUGINS=1
ENV QT_PLUGIN_PATH="/opt/conda/envs/cellpose/lib/python3.8/site-packages/PyQt6/Qt6/plugins/platforms"

RUN conda install mamba -n base -c conda-forge
RUN mamba create -y --name cellpose python==3.8.5 pytorch=1.10.1  torchvision==0.11.2 torchaudio==0.10.1 cudatoolkit=11.3 mkl==2023.0.0 -c pytorch -c conda-forge
RUN conda run --name cellpose python -m pip install cellpose[all]==2.3.2
RUN conda run --name cellpose python -m pip install pyqt5 numpy>=1.20.0 numba>=0.43.1 scipy opencv-python-headless pyqtgraph>=0.11.0rc0 natsort tqdm tifffile fastremap roifile 


RUN mkdir -p /run/user
RUN chmod 777 /run/user

USER root

RUN mkdir -p /etc/localaccount
RUN for f in passwd shadow group gshadow ; do if [ -f /etc/$f ] ; then  cp /etc/$f /etc/localaccount; rm -f /etc/$f; ln -s /etc/localaccount/$f /etc/$f; fi; done


RUN echo '#!/usr/bin/env -S bash --noprofile --norc\nsource /opt/conda/etc/profile.d/conda.sh\nconda activate cellpose\nexport QT_PLUGIN_PATH=/opt/conda/envs/cellpose/lib/python3.8/site-packages/PyQt5/Qt5/plugins/platforms\npython -m cellpose "$@"' > /usr/local/bin/cellpose && \
  chmod +x /usr/local/bin/cellpose 



CMD ["/composer/appli-docker-entrypoint.sh" ]


