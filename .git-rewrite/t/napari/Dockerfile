ARG TAG=3.0
FROM abcdesktopio/oc.template.ubuntu.20.04:$TAG

ENV DEBIAN_FRONTEND noninteractive
ENV TZ Europe/Berlin
ENV QT_XCB_NO_MITSHM=1 
#ENV LD_LIBRARY_PATH "/usr/local/nvidia/lib:/usr/local/nvidia/lib64",
ENV NVIDIA_VISIBLE_DEVICES "all",
ENV NVIDIA_DRIVER_CAPABILITIES "compute,utility"

RUN apt-get update -y && apt-get install -qqy build-essential 

RUN apt-get install -qqy wget mesa-utils \
                         libgl1-mesa-glx \
                         libglib2.0-0 \
                         libfontconfig1 \
                         libxrender1 \
                         libdbus-1-3 \
                         libxkbcommon-x11-0 \
                         libxi6 \
                         libxcb-icccm4 \
                         libxcb-image0 \
                         libxcb-keysyms1 \
                         libxcb-randr0 \
                         libxcb-render-util0 \
                         libxcb-xinerama0 \
                         libxcb-xinput0 \
                         libxcb-xfixes0 \
                         libxcb-shape0 \
                         fonts-dejavu \
                         fontconfig

COPY environment.yaml /tmp

WORKDIR /tmp
RUN wget https://repo.anaconda.com/miniconda/Miniconda3-latest-Linux-x86_64.sh \
    && bash Miniconda3-latest-Linux-x86_64.sh -b -p /opt/conda \
    && rm -f Miniconda3-latest-Linux-x86_64.sh 


LABEL oc.icon="napari.svg"
LABEL oc.icondata=""
LABEL oc.keyword="napari"
LABEL oc.cat="development"
LABEL oc.template="abcdesktopio/oc.template.ubuntu.22.04"
LABEL oc.name="napari"
LABEL oc.launch="napari.napari"
LABEL oc.displayname="napari"
LABEL oc.path="/opt/conda/envs/napari/bin/napari"
LABEL oc.type=app
LABEL oc.showinview=dock
LABEL oc.rules="{\"homedir\":{\"default\":true}}"
LABEL oc.acl="{\"permit\":[\"all\"]}"
LABEL oc.containerengine="pod_application"
LABEL oc.executeclassname="imaging"


RUN  if [ -d /usr/share/icons ];   then cd /usr/share/icons;    /composer/safelinks.sh; fi 
RUN  if [ -d /usr/share/pixmaps ]; then cd /usr/share/pixmaps;  /composer/safelinks.sh; fi 

ENV APPNAME "napari"
ENV APPBIN "/opt/conda/envs/napari/bin/napari"
ENV APP "/opt/conda/envs/napari/bin/napari"

ENV CONDA_BIN_PATH="/opt/conda/bin"
ENV PATH $CONDA_BIN_PATH:$PATH

RUN conda install mamba -n base -c conda-forge && \
    mamba env create -f /tmp/environment.yaml


RUN mkdir -p /run/user
RUN chmod 777 /run/user

USER root
RUN mkdir -p /etc/localaccount
RUN for f in passwd shadow group gshadow ; do if [ -f /etc/$f ] ; then  cp /etc/$f /etc/localaccount; rm -f /etc/$f; ln -s /etc/localaccount/$f /etc/$f; fi; done


CMD /opt/conda/envs/napari/bin/napari

