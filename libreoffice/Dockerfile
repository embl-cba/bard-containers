ARG TAG=3.2
FROM abcdesktopio/oc.template.ubuntu.22.04:$TAG

USER root

ENV LANG=C.UTF-8 LC_ALL=C.UTF-8
ENV NVIDIA_DRIVER_CAPABILITIES "compute,utility"

RUN DEBIAN_FRONTEND=noninteractive apt-get update -y && apt-get install -y bzip2 wget vim ca-certificates curl firefox software-properties-common

RUN add-apt-repository ppa:libreoffice/ppa

RUN DEBIAN_FRONTEND=noninteractive apt-get update -y && apt-get install -y libreoffice

LABEL oc.icon="libre.svg"
LABEL oc.icondata=""
LABEL oc.keyword="libreoffice"
LABEL oc.cat="office"
LABEL oc.template="abcdesktopio/oc.template.ubuntu.22.04"
LABEL oc.name="lireoffice"
LABEL oc.launch="libreoffice.libreoffice-startcenter "
LABEL oc.displayname="LibreOffice"
LABEL oc.path="/usr/bin/libreoffice"
LABEL oc.type=app
LABEL oc.rules="{\"homedir\":{\"default\":true}}"
LABEL oc.showinview=dock
LABEL oc.acl="{\"permit\":[\"all\"]}"
LABEL oc.executeclassname="office"

RUN  if [ -d /usr/share/icons ];   then cd /usr/share/icons;    /composer/safelinks.sh; fi 
RUN  if [ -d /usr/share/pixmaps ]; then cd /usr/share/pixmaps;  /composer/safelinks.sh; fi 

ENV APPNAME "libreoffice"
ENV APPBIN "/usr/bin/libreoffice"
ENV APP "/usr/bin/libreoffice"

RUN mkdir -p /run/user
RUN chmod 777 /run/user
USER root
RUN mkdir -p /etc/localaccount
RUN for f in passwd shadow group gshadow ; do if [ -f /etc/$f ] ; then  cp /etc/$f /etc/localaccount; rm -f /etc/$f; ln -s /etc/localaccount/$f /etc/$f; fi; done



CMD ["/composer/appli-docker-entrypoint.sh" ]


