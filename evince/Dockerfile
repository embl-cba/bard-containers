FROM abcdesktopio/oc.template.ubuntu.22.04:3.0

ENV DEBIAN_FRONTEND=noninteractive

# Install dependencies
RUN apt-get update && apt-get install -y \
    evince \
    x11-apps \
    dbus-x11 \
    libglib2.0-0 \
    libgtk-3-0 \
    && apt-get clean && rm -rf /var/lib/apt/lists/*

LABEL oc.icon="evince.svg"
LABEL oc.icondata=""
LABEL oc.keyword="evince,pdf, viewer"
LABEL oc.cat="office"
LABEL oc.desktopfile="evince.desktop"
LABEL oc.launch="evince.Evince"
LABEL oc.template="abcdesktopio/oc.template.ubuntu"
LABEL oc.name="evince"
LABEL oc.displayname="Evince PDF Viewer"
LABEL oc.path="/usr/bin/evince"
LABEL oc.type=app
LABEL oc.mimetype="application/pdf;"
LABEL oc.fileextensions="pdf"
LABEL oc.legacyfileextensions="pdf"
LABEL oc.rules="{\"homedir\":{\"default\":true}}"
LABEL oc.acl="{\"permit\":[\"all\"]}"
LABEL oc.showinview="dock"

ENV APPNAME "evince"
ENV APPBIN "/usr/bin/evince"
# ENV APP is deprecated, removed in next release

ENV APP "/usr/bin/evince"

RUN mkdir -p /etc/localaccount
RUN for f in passwd shadow group gshadow ; do if [ -f /etc/$f ] ; then  cp /etc/$f /etc/localaccount; rm -f /etc/$f; ln -s /etc/localaccount/$f /etc/$f; fi; done

CMD [ "/composer/appli-docker-entrypoint.sh" ]
