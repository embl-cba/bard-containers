ARG TAG=3.2
FROM registry.git.embl.de/grp-cbbcs/abcdesktop-apps/base-image:ubuntu22-cuda-11-8
USER root

ENV LANG=C.UTF-8 LC_ALL=C.UTF-8
ENV NUMBA_CACHE_DIR=/tmp/numba_cache
ENV QT_XCB_NO_MITSHM=1
ENV NVIDIA_DRIVER_CAPABILITIES "compute,utility"
ENV CONDA_BIN_PATH="/opt/conda/bin"
ENV PATH $CONDA_BIN_PATH:$PATH
ENV BIOIMAGEIO_CACHE_PATH="/tmp/$CURRENT_USER/pybio_cache"
ENV CURRENT_USER=`whoami`
ENV LAZYFLOW_THREADS=2
ENV LAZYFLOW_TOTAL_RAM_MB=32000

RUN DEBIAN_FRONTEND=noninteractive apt-get update -y && apt-get install -y bzip2 wget ca-certificates curl firefox

RUN echo 'debconf debconf/frontend select Noninteractive' | debconf-set-selections

RUN DEBIAN_FRONTEND=noninteractive apt-get install -y build-essential \
	libglu1-mesa-dev \
	libgl1-mesa-glx \
	nautilus \
        xdg-utils \
        wget \
        konsole


WORKDIR /opt
  
RUN wget https://files.ilastik.org/ilastik-1.4.1b20-gpu-Linux.tar.bz2 && \
    tar xjf ilastik-1.*-Linux.tar.bz2 && \
    rm -f ilastik-1.*-Linux.tar.bz2


LABEL oc.icon="ilastik-logo.svg"
LABEL oc.icondata=""
LABEL oc.keyword="ilastik"
LABEL oc.cat="development"
LABEL oc.template="abcdesktopio/oc.template.ubuntu.22.04"
LABEL oc.name="ilastik"
LABEL oc.desktopfile="org.kde.konsole.desktop"
LABEL oc.launch="ilastik.ilastik"
LABEL oc.displayname="ilastik v1.4.1b20-gpu"
LABEL oc.path="/opt/ilastik-1.4.1b20-gpu-Linux/run_ilastik.sh"
LABEL oc.type=app
LABEL oc.rules="{\"homedir\":{\"default\":true}}"
LABEL oc.showinview=dock
LABEL oc.acl="{\"permit\":[\"all\"]}"
LABEL oc.executeclassname="imaging"

RUN  if [ -d /usr/share/icons ];   then cd /usr/share/icons;    /composer/safelinks.sh; fi 
RUN  if [ -d /usr/share/pixmaps ]; then cd /usr/share/pixmaps;  /composer/safelinks.sh; fi 

ENV APPNAME "ilastik"
ENV APPBIN "/opt/ilastik-1.4.1b20-gpu-Linux/run_ilastik.sh"
ENV APP "/opt/ilastik-1.4.1b20-gpu-Linux/run_ilastik.sh"

RUN mkdir -p /run/user
RUN chmod 777 /run/user
USER root
RUN mkdir -p /etc/localaccount
RUN for f in passwd shadow group gshadow ; do if [ -f /etc/$f ] ; then  cp /etc/$f /etc/localaccount; rm -f /etc/$f; ln -s /etc/localaccount/$f /etc/$f; fi; done

CMD ["sh", "-c","mkdir -p ~/tmp && /composer/appli-docker-entrypoint.sh " ]


